{
  "navigationFallback": {
    "rewrite": "/404.html",
    "exclude": ["/images/*", "/favicon.svg", "/_next/*", "/css/*", "/js/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "globalHeaders": {
    "cache-control": "no-cache"
  },
  "routes": [
    {
      "route": "/images/*",
      "headers": {
        "cache-control": "public, max-age=604800"
      }
    }
  ]
}
